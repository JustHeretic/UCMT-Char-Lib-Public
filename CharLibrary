-- Characters in Box name change
if BoxAlreadyChanged ~= true then
for i,v in pairs(game:GetService("Workspace").Box:GetChildren()) do
for i2,v2 in pairs(v:GetChildren()) do
CharBoxCounter = 0
    if v2.Name == "Rigged Morph Template" or v2.Name == "Head" then
    v.Name = v.Name..CharBoxCounter
    end
end
end
getgenv().BoxAlreadyChanged = true
end

--------------- [Donate Character Section] ---------------
function ShadowSans()
local character = game:GetService("Workspace").GamepassCharacter.ShadowSans.Head
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, character, 0)
wait()
firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, character, 1)
end

